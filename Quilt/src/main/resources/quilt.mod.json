{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "${mod_id}",
    "version": "${version}",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "client_init": [
        "com.christofmeg.justenoughbreeding.JustEnoughBreeding"
      ],
      "jei_mod_plugin": [
        "com.christofmeg.justenoughbreeding.jei.JEIPlugin"
      ],
      "rei_client": [
        "com.christofmeg.justenoughbreeding.rei.REIPlugin"
      ],
      "emi": [
        "com.christofmeg.justenoughbreeding.emi.EMIPlugin"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "jei",
        "version": ">=15.20",
        "optional": true
      }
    ],
    "metadata": {
      "name": "${mod_name}",
      "description": "${description}",
      "contributors": {
        "${mod_author}": "Developer"
      },
      "contact": {
        "homepage": "https://github.com/Christofmeg/JustEnoughBreeding",
        "sources": "https://github.com/Christofmeg/JustEnoughBreeding",
        "issues": "https://github.com/Christofmeg/JustEnoughBreeding/issues"
      },
      "license": "MIT",
      "icon": "icon.png"
    }
  }
}