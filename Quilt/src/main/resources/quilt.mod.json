{
    "schema_version": 1,
    "quilt_loader": {
        "group": "com.christofmeg.justenoughbreeding",
        "id": "justenoughbreeding",
        "version": "${version}",
        "intermediate_mappings": "net.fabricmc:intermediary",
        "entrypoints": {
          "client_init": [
		    "com.christofmeg.justenoughbreeding.JustEnoughBreeding"
		  ],
		  "jei_mod_plugin": [
		    "com.christofmeg.justenoughbreeding.jei.JEIPlugin"
		  ]
        },
        "depends": [
            {
				"id": "quilt_loader",
				"version": "*"
			},
			{
				"id": "quilt_base",
				"version": "*"
			},
			{
				"id": "forgeconfigapiport",
				"version": ">=7.0.0"
			}
        ],
        "metadata": {
            "name": "Just Enough Breeding",
            "description": "JEI plugin that displays breeding information",
            "contributors": {
                "Christofmeg": "Developer"
            },
            "contact": {
				"homepage": "https://github.com/Christofmeg/JustEnoughBreeding",
				"sources": "https://github.com/Christofmeg/JustEnoughBreeding",
				"issues": "https://github.com/Christofmeg/JustEnoughBreeding/issues"
            },
            "license": "MIT",
            "icon": "icon.png"
        }
    }
}